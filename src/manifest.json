{

  "description": "This extension is used to display and search for bookmarks.",
  "manifest_version": 2,
  "name": "Bookmarks",
  "version": "1.0",
  "permissions": ["bookmarks"],
  "chrome_url_overrides": {
		"newtab": "index.html"
	},
	"chrome_settings_overrides": {
		"homepage": "index.html"
	},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
